# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true
charset = utf-8
trim_trailing_whitespace = true

[*]
indent_style = tab
indent_size = 4
end_of_line = lf
insert_final_newline = true

[*.md]
indent_style = space
trim_trailing_whitespace = false

[*.yml]
indent_style = space

[*.json]
indent_style = space
